{
  "version": 2,
  "name": "samadhanhub",
  "alias": [
    "samadhanhub.com"
  ],
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "client-agnivirya/dist",
  "installCommand": "npm run install:all",
  "functions": {
    "server/index.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client-agnivirya/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "APP_MODE": "prod",
    "ENVIRONMENT": "vercel",
    "PROD_DOMAIN": "samadhanhub.com"
  }
}